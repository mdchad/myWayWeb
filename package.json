{
  "name": "t3-turbo-and-clerk",
  "private": true,
  "packageManager": "pnpm@7.13.0",
  "scripts": {
    "build": "turbo build",
    "clean": "rm -rf node_modules",
    "clean:workspaces": "turbo clean",
    "db-generate": "turbo db-generate",
